<?xml version="1.0"?>
<!-- Tasks to clean generated files after build -->

<project name="freeguide-tv-cleanup" default="clean">
    <!-- import the variables -->
    <import file="../build.xml"/>

    <target name="clean-properties">
        <delete>
            <fileset dir="${resources}" includes="*.properties"/>
        </delete>
    </target>

    <target name="clean" depends="clean-properties">
        <delete dir="${build}"/>
        <delete dir="${dist}"/>
        <delete dir="${run}"/>
    </target>

</project>