<?xml version="1.0"?>

<project name="freeguide-tv-dist-rpm" default="dist-rpm">
    
	<target name="source-all">
        <ant antfile="build-source.xml" />
    </target>
    
	<target name="dist-rpm" depends="source-all">
		<exec os="linux" dir="${src}/install/linux/rpm" executable="buildrpm" resolveexecutable="true" failonerror="true"/>
	</target>
    
</project>

